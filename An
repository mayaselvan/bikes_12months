/* Step 4: Analyze


##### Total Trips by User Type #####
SELECT
      member_casual,
      COUNT(*) AS total_trips,
      COUNT(*) * 100 / SUM(COUNT(*)) OVER () AS percent_of_total
FROM 
      cyclistic-24-25.cyclistic_dataset.bikes_12months
GROUP BY 
      member_casual;


##### Ride Frequency by Month (Seasonality) #####
SELECT
      member_casual,
      COUNT(*) AS total_trips,
      COUNT(*) * 100 / SUM(COUNT(*)) OVER () AS percent_of_total
FROM 
      cyclistic-24-25.cyclistic_dataset.bikes_12months
GROUP BY 
      member_casual;
